<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <Version>1.0.0</Version>
    <Authors>Antti Alasvuo</Authors>
    <PackageProjectUrl>https://github.com/alasvant/</PackageProjectUrl>
    <RepositoryUrl>https://github.com/alasvant/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>Logging,Audit</PackageTags>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Copyright>Antti Alasvuo Â©2020</Copyright>
    <Description>Log changes to content access rights to Episerver activity log (change log).</Description>
    <PackageReleaseNotes>Initial release.</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="lang\AuditLogging.xml" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="lang\AuditLogging.xml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="EPiServer.CMS.AspNet" Version="[11.1.0,12.0)" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web" />
  </ItemGroup>

</Project>
